<div ng-repeat="vote in votes">
  <h3> {{vote.title}}</h3>

  <div ng-repeat="selection in vote.selections | orderBy: '-points'">
    {{selection.selection_title}}: {{selection.points}}

    <!-- if owner of vote need to have add selection or edit or delete -->
    <button ng-click="upvote(selection)">Upvote</button>
  </div>
</div>
<div>
<button ng-click="go('/votes')" type="button" class="btn btn-default btn-lg">
  <span class="glyphicon glyphicon-star"></span> Votes
</button>
<button ng-click="go('/new_vote')" type="button" class="btn btn-default btn-lg">
  <span class="glyphicon glyphicon-star"></span> New Vote
</button>
</div>